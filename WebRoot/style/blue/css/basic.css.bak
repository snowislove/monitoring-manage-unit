/************************************wrong start***************************************/
.form_main{
	margin:0 auto;
	padding-left:100px;
	padding-top:20px;	
	font:12px/25px "宋体" ;
	text-align:left;
	overflow:hidden;
}
.form_main td{
    line-height:28px;
	padding:0;
	margin:0;
	border-right:none;border-left:none;
	border-bottom:1px #eeecf2 solid
}
.tips{
	background:#f8f9ed url(../images/tips.png) no-repeat;
	font-family:"宋体";
	font-size: 12px;
	font-weight: bold;
	color: #333333;
	text-indent: 25px;
	line-height:26px;
}
.label{
   text-align:right;
   width:30%;
}
.label span,.tips span{
    color:#ff0000;
}
.desc{
   width:30%;    
}
.error{
	background:#fffae3;
	border:1px solid #ff7200;
	width:222px;
	height:17px;
	text-align:left;
	display:block;
	padding:0 5px;
}
.error span{
    background:url(../images/err.png) no-repeat;
	padding-left:23px;
	line-height:18px;
	color:#862512;
	display:block
}
/*popup error*/
.closewindow{
    background:url(../images/close.png) no-repeat;
	width:15px;
	height:15px;
	padding-right:5px;
	float:right;
	margin-top:-20px;
	cursor:pointer;
}
.closewindow_body{
    overflow:auto;
	width:100%;
	text-align:center; 
	height:100%;
	background:url(../images/closewindow_body_bg.png) repeat-x bottom;
	}
.error_main{
	margin:0;
	padding-left:100px;
	padding-top:20px;
	background:url(../images/tip_wrong.gif) no-repeat 45px 20px;
	font:12px/25px "宋体" ;
	text-align:left;
	overflow:hidden;
}
/*popup error*/	
.button_bar{
    height:28px;
	line-height:28px;
	overflow:hidden;
	margin-top:15px;
	padding-top:5px;
	border-top: 1px dashed #a1a1a1;
	text-align:center;
}
/**************************************wrong end***************************************/


/***************************welcome start****************************************/

.welcome_content{
	background:#BDD5FC url(../images/welcome_bg.png) no-repeat center top;
}
/*****************************welcome End***************************************/


/*********************************waitting start************************************/
.waitting_content{
	background: #BDD5FC url(../images/waitting_bg.png) center;
}
.wait_word{
	height:190px;
	width:271px;
	background: url(../images/waitting.gif) no-repeat;
	padding-left:100px;
	margin:130px auto;
	line-height:75px;
	font-size:14px;
	color:#01075b;
}
.waitting_popup{
	margin:0;
	padding-left:100px;
	padding-top:40px;
	background:url(../images/waitting_2.gif) no-repeat 34px 40px;
	font:12px/34px "宋体" ;
	text-align:left;
	overflow:hidden;
	}
/***********************************waitting end***************************************/


/*************************************add&del&edit end*********************************/
.confirm_main{
	margin:0;
	padding-left:20px;
	background:url(../images/alarm.png) no-repeat 45px 20px;
	font:12px/85px "宋体" ;
}
/***********************************add&del&edit end************************************/



/***********************************register start**************************************/
.guide{
    width:100%;
	background:url(../images/guide_bar.png) repeat-x top;
	padding:0px;
	height:28px;
	text-align:center;
	padding-left:0px!important;
	*padding-left:25%!important;
	padding-left:25%;
	}
.guide span{
    display:block;
	height:28px;
	width:100px;
	line-height:40px;
	float:left;
}
.steparrow{
    background:url(../images/guide_arrow.png) no-repeat center 10px;
	margin:5px 5px;
}
.current{
   background:url(../images/guide_current.png) no-repeat center 3px;
   padding:0;
   color:#184d97;
   font-weight:bold;
}
/**********************************register end***************************************/


/********************************** popupTip start*************************************/
.popupTip{
	width: 330px;
	height: 230px;
	border-bottom:#90b4e5 1px solid;
	border-left: #90b4e5 1px solid;
	border-right: #90b4e5 1px solid;
	font-size:12px;
	font-weight:bold;
	font-family:Arial, Helvetica, sans-serif,"宋体";
	Z-INDEX: 10; 
	VISIBILITY: hidden; 
	POSITION: absolute;
}
.popup_title{
	background:url(../images/content_title_bg.png) repeat-x;
	height:26px;
	width:100%;	
	line-height:26px;
}
.popup_title .text{
	padding-left:10px;
	color: #184d97;
}
.Popup_content{
	background:#FFFFFF;
	width:100%;
	height:174px;
}
.Popup_content_tabel{
	font-size: 12px;
	color: #333333;
	width:100%;
	position:relative;
	background:#FFFFFF;
	border:none;
}
.Popup_content_tabel td{
    background:#FFFFFF;
	border:none;
}
/*********************************** popupTip end**************************************/

/*************************************configure****************************************/
.left_content{
	width:100%;
	overflow:auto;
	height:350px;
	}
.left_content ul{
	list-style-type:none;
	margin:10px 0px 0px -40px !important;
	*margin:10px 0px 0px 0px !important;
}
.left_content ul li{
	padding:5px 0 7px 10px;
	cursor:pointer;
}
.left_content ul li.active{
	background:url(../images/left_content_activebg.png) repeat-x center center;
}
.left_content ul li img{
	margin:1px 6px -3px 0px;
}
.content_tabcontent{
	margin:10px 0 0 15px;
	}
.content_tabcontent .textinfo{
	width:120px;
	padding:10px;
	margin:5px;
	text-align:right;
	width:130px;
}
/*********************************configure*************************************/

