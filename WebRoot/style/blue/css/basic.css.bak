/************************************ search css *************************************/
.search_sitebg {	
	height:100px;
	margin:0;
	padding:0;
	clear:both;
}
.serach_main{
	margin:0;
	padding:0;
}
.search_main td{
    border:none;
	margin:0px;
	padding:0px;
	line-height:28px;
}
.search_main td.label{
    text-align:right;
}
.search_main td.label span{
    color:#f00;
}
.search_main td.input{
    text-align:left;
}
.searchTitle{
	width:100%;
	background:#f8f9ed;
	font-weight: bold;
	color: #333333;
	text-indent: 25px;
	border-bottom:1px dotted #757575;
	text-align:left;
	line-height:26px;
}
.search_close {
   height:20px;
   width:20px;
   cursor:pointer;
   margin-top:-20px;
   float:right;
   background:url(../images/close.png) no-repeat left top;
}
/********************************* search css ********************************/

