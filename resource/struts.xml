		<action name="resetPasswd" class="UserAction" method="resetPasswd">	
			<result name="success">/login.jsp</result>
			<result name="resetError">/users/resetPasswd.jsp</result>
		</action>
		<action name="checkPasswd" class="UserAction" method="checkPasswd">	
		</action>
		<!-- 密码策略 -->
		<action name="toModifyPasswdRule" class="SysPasswdRuleAction" method="toModifyPasswdRule">	
			<result name="success">/users/modifyPasswdRule.jsp</result>
			<result name="failed">/error.jsp</result>
		</action>
		<action name="modifyPasswdRule" class="SysPasswdRuleAction" method="modifyPasswdRule">	
			<result type="redirectAction" name="success">toModifyPasswdRule</result>
			<result name="failed">/error.jsp</result>
		</action>
		<!-- 用户访问策略 -->
		<action name="toModifyAccessRule" class="SysAccessRuleAction" method="toModifyAccessRule">	
			<result name="success">/users/modifyAccessRule.jsp</result>
			<result name="failed">/error.jsp</result>
		</action>
		<action name="modifyAccessRule" class="SysAccessRuleAction" method="modifyAccessRule">	
			<result type="redirectAction" name="success">toModifyAccessRule</result>
			<result name="failed">/error.jsp</result>
		</action>
		<!-- 系统日志 -->
		<action name="viewLog" class="LogAction" method="viewLog">	
		</action>

		<!--调度任务 -->
		<action name="*TaskScheduleAction" class="TaskScheduleAction" method="{1}">
			<result name="close" type="dispatcher">/task/closed.jsp</result>
			<result name="failed">/error.jsp</result>
			<result name="success">/task/taskView.jsp</result>
			<result name="toView">/task/taskView.jsp</result>
		</action>
		<!-- 任务执行情况 -->
		<action name="viewTaskExecute" class="MsuSchedUuleAction" method="viewTaskExecute">	
		</action>
		<!-- MD5完整性校验 -->
		<action name="md5Receive" class="Md5RecAction" method="md5Receive">	
		</action>
	</package>
	<constant name="struts.action.excludePattern" value="/dwr/.*,/dwr/test/.*" />
</struts>    
